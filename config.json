{
	"config": {
		"mw": {
			"conn": {
				"username": "mw-username",
				"password": "mw-passwd",
				"server": "wiki.example.com",
				"path": "/w"
			},
			"smwquery": {
				"proteins": {
					"base": "[[Category:Proteins]]",
					"fields": [
						"Page ID", "Revision ID", "Name", "Comment", "Id", "Class"
					]
				}
			},
			"recent-changes": {
				"proteins": {
					"base": "[[Category:Proteins]]"
				}
			}
		},
		"target": {
			"type": "couchdb",
			"params": {
				"username": "couch-user",
				"password": "couch-passwd",
				"host": "couch.example.com",
				"port": 80,
				"db": "mydb"
			},
			"document": {
				"proteins": {
					"_id": "Page ID",
					"revid": "$Revision ID",
					"mwid": "Id",
					"name": "Name",
					"class": "Class",
					"comment": "Comment",
					"pagename": "#fulltext",
					"ns": "$#namespace"
				}
			},
			"indexes": {
				"_design/refs": {
					"language": "javascript",
					"views": {
	
					}
				},
				"_design/tal": {
					"language": "javascript",
					"views": {
	
					}
				},
				"_design/search" : {
					"fulltext": {
						"by_text": {
							"index":"function(doc) { var ret=new Document(); ret.add(doc.comment); ret.add(doc.name);  return ret }"
						}
					}, "views": {}
				}
			}
		}
	}
}
