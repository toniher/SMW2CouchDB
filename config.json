{
	"config": {
		"mw": {
			"username": "MycoWiki",
			"password": "",
			"host": "mycowiki.crg.eu",
			"smwquery": {
				"base": "[[Category:xxx]]",
				"fields": [
					"Page_ID", "Revision_ID", "Name", "Comment", "Id", "Class"
				]
			}
		},
		"target": {
			"type": "couchdb",
			"params": {
				"username": "mycowiki",
				"password": "",
				"host": "http://couchdb.prgdb.crg.eu"
			},
			"document": {
				"_id": "@Page_ID",
				"revid": "@Revision_ID",
				"mwid": "@Id",
				"name": "@Name",
				"class": "@Class",
				"comment": "@Comment"
			},
			"indexes": {
				"_design/refs.js": {
					"language": "javascript",
					"views": {
	
					}
				},
				"_design/tal.js": {
					"language": "javascript",
					"views": {
	
					}
				},
				"_design/foo" : {
					"fulltext": {
						"by_subject": {
							"index":"function(doc) { var ret=new Document(); ret.add(doc.subject); return ret }"
						},
						"by_content": {
							"index":"function(doc) { var ret=new Document(); ret.add(doc.content); return ret }"
						}
					}
				}
			}
		}
	}
}
